<h1>All my movie lists 🍿 </h1>

<h2>My lists</h2>


<div class="cards">
  <% @lists.each do |list| %>
    <div class="card-category"
    style="linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), background-image: url('https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/breakfast.jpg')">
    <%= link_to list.name, list_path(list) %>
    </div>
  <% end %>
</div>

<p><%= link_to "Create a movie list", new_list_path() %></p>


